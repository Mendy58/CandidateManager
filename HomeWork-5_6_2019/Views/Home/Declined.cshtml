@using HomeWork_5_6_2019.Data
@model IEnumerable<Candidate>

<div class="well">
	<table class="table table-striped table-hover">
		<tr>
			<th></th>
			<th>Name</th>
			<th>Phone Number</th>
			<th>Email</th>
		</tr>
		@foreach (Candidate c in Model)
		{
			<tr>
				<td><a href="/home/details?id=@c.id">View Details</a></td>
				<td>@c.Name</td>
				<td>@c.PhoneNumber</td>
				<td>@c.Email</td>
			</tr>
		}
	</table>
</div>

